{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "required": [
        "code",
        "meta",
        "data"
    ],
    "properties": {
        "code": {
            "$id": "#code",
            "type": "integer"
        },
        "meta": {
            "$id": "#meta",
            "type": "null"
        },
        "data": {
            "$id": "#data",
            "type": "object",
            "required": [
                "id",
                "name",
                "email",
                "gender",
                "status"
            ],
            "properties": {
                "id": {
                    "$id": "#id",
                    "type": "integer"
                },
                "name": {
                    "$id": "#name",
                    "type": "string"
                },
                "email": {
                    "$id": "#email",
                    "type": "string"
                },
                "gender": {
                    "$id": "#gender",
                    "type": "string"
                },
                "status": {
                    "$id": "#status",
                    "type": "string"
                }
            },
            "additionalProperties": true
        }
    },
    "additionalProperties": true
}